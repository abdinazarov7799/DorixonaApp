{
  "expo": {
    "name": "Dorixona",
    "slug": "jurabek-laboratories-mobile",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/greenLittleMe.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "@/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#215ca0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.diyorbekabdinazarov.xbsendmemobile",
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/greenLittleMe.png",
        "backgroundColor": "#215ca0"
      },
      "package": "com.diyorbekabdinazarov.xbsendmemobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/greenLittleMe.png"
    },
    "plugins": [
      "expo-router",
      "expo-screen-orientation",
      "@config-plugins/react-native-blob-util",
      "@config-plugins/react-native-pdf"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "be6d7146-b659-449b-8c9b-477e04bfd3dd"
      }
    }
  }
}
